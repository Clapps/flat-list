{
  "name": "NoGrey",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "rimraf ./node_modules/react-native/local-cli/core/__fixtures__/files/package.json",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:clean": "npm run start -- --reset-cache",
    "lint": "eslint ./app --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "flow": "glow",
    "devtools": "react-devtools",
    "ios": "cross-env ENVFILE=.env.ios react-native run-ios",
    "android": "cross-env ENVFILE=.env.android react-native run-android",
    "build:ios:assets": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build:android:assets": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build:android:test": "cd android && cross-env ENVFILE=.env.test ./gradlew assembleRelease",
    "build:android:staging": "cd android && cross-env ENVFILE=.env.staging ./gradlew assembleRelease",
    "build:android:production": "cd android && cross-env ENVFILE=.env.production ./gradlew assembleRelease",
    "build:staging_debug": "bundle exec fastlane android staging_debug",
    "deploy:android": "bundle exec fastlane android alpha",
    "deploy:android:internal": "bundle exec fastlane android internal",
    "deploy:ios": "bundle exec fastlane ios TestFlight",
    "android:test": "cross-env ENVFILE=.env.test react-native run-android",
    "android:staging": "cross-env ENVFILE=.env.staging react-native run-android",
    "android:production": "cross-env ENVFILE=.env.production react-native run-android",
    "test:update": "jest --u",
    "test": "jest --verbose",
    "rename": "node ./scripts/rename-android.js"
  },
